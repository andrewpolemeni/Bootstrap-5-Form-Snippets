	"Bootstrap 5: Blank Form Group": {
		"prefix": "*fg-blank",
		"body": [
			"<div class=\"mb-3\">",
			"    <label id=\"$1_label\" for=\"$1\">${2}</label>",
			"    <input id=\"$1\" name=\"$1\" type=\"text\" class=\"form-control\" aria-label=\/${2}\" />",
			"</div>"
		],
		"description": "Blank Form Group"
	},
