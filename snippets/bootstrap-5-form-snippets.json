	"Bootstrap 5: Select": {
		"prefix": "*fg-select",
		"body": [
			"<div class=\"select\">",
			"    <label id=\"$1_label\" for=\"$1\">$2</label>",
			"    <div class=\"input-group mb-3\">",
			"        <select id=\"$1\" name=\"$1\" class=\"form-control form-select\" aria-label=\"$2\">",
            "            <option></option>",
            "            <option>$3</option>",
            "            <option>$4</option>",
        	"        </select>",
			"    </div>",
			"</div>"
		],
		"description": "Bootstrap Select Form Field"
	},
	"Bootstrap 5: File Upload": {
		"prefix": "*fg-file-upload",
		"body": [
			"<div class=\"file-upload\">",
			"    <label for=\"file_upload\">File Upload</label>",
			"    <div class=\"custom-file\">",
			"        <div class=\"input-group mb-3\">",
            "            <input id=\"file_upload\" name=\"file_upload\" type=\"file\" class=\"custom-file-input form-control\" aria-label=\"File Upload\" />",
        	"        </div>",
			"    </div>",
			"</div>"
		],
		"description": "Bootstrap File Form Field"
	},
	"Bootstrap 5: Amount": {
		"prefix": "*fg-amount",
		"body": [
			"<div class=\"amount\">",
			"    <label id=\"amount_label\" for=\"amount\">Amount</label>",
			"    <div class=\"input-group mb-3\">",
			"        <div class=\"input-group-text\">$</div>",
            "        <input id=\"amount\" name=\"amount\" type=\"number\" class=\"form-control\" value=\"0.00\" aria-label=\"Total Amount\" />",
        	"    </div>",
			"</div>"
		],
		"description": "Bootstrap Amount Form Field"
	},
	"Bootstrap 5: Radio Button Form Check": {
		"prefix": "*fc-radio-button",
		"body": [
			"<div class=\"form-check mb-3\">",
			"    <input id=\"$1\" name=\"$1\" type=\"radio\" class=\"form-check-input\" value=\"$3\" />",
			"    <label id=\"$1_label\" class=\"form-check-label\" for=\"$1\">$2</label>",
			"</div>"
		],
		"description": "Bootstrap Radio Button Form Check"
	},
